# Testing print(), puts() & print2str() inside functions

puts("Hello, World!")
name= "Ring"
puts("Using #{name} language")

test()

func test
	puts("Hello, World!")
	name2= "**Ring**"
	puts("Using #{name2} language")
	? print2str("Using #{name2} language")
	? "I love Programming!"
	print("Using #{name2} language")